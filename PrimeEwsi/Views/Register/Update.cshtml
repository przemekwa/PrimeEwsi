@model PrimeEwsi.Models.UserModel

@{
    ViewBag.Title = "Update";
   }

 <div class="panel panel-primary" style="width: 50%; margin-top: 20px">
    <div class="panel-heading">
        <h3 class="panel-title">User profile update</h3>
    </div>
    <div class="panel-body">
        
        @using (Html.BeginForm("Update", "Register"))
        {
            @Html.ValidationSummary()

            <p>Nick @Html.EditorFor(m => m.Name, new { value=@Model.Name })</p>

            <p>Api Key @Html.EditorFor(m => m.ApiKey)</p>

            <p>SVN User Name @Html.EditorFor(m => m.SvnUser)</p>

            <p>SVN Password  @Html.EditorFor(m => m.SvnPassword)</p>

            <input type="submit" value="Update" />
        }
    </div>
</div>