@model PrimeEwsi.Models.UserModel

@{
    ViewBag.Title = "Update";
   }

 <div class="panel panel-primary" style="width: 50%; margin-top: 20px">
    <div class="panel-heading">
        <h3 class="panel-title">User profile update</h3>
    </div>
    <div class="panel-body">
        
        @using (Html.BeginForm("Update", "Register"))
        {
            @Html.ValidationSummary()


            <div class="form-group">
                <div class="form-group">
                    <label for="usr">Name</label>
                    <input class="form-control" id="UserName" name="UserName" type="text" value="@Model.UserName" />
                </div>
            </div>

            <div class="form-group">
                <div class="form-group">
                    <label for="usr">Artifactory API key</label>
                    <input class="form-control" id="UserApiKey" name="UserApiKey" type="text" value="@Model.UserApiKey"/>
                    
               
                    <a href="https://ewsi.centrala.bzwbk:9999/artifactory/webapp/#/login" target="_blank">Get Artifactory API key</a>
                </div>
            </div>


            <div class="form-group">
                <div class="form-group">
                    <label for="usr">SVN user</label>
                    <input class="form-control" id="SvnUser" name="SvnUser" type="text" value="@Model.SvnUser"/>
                    
                </div>
            </div>


            <div class="form-group">
                <div class="form-group">
                    <label for="usr">SVN Password</label>
                    <input class="form-control" id="SvnPassword" name="SvnPassword" type="password" value="@Model.SvnPassword"/>

                </div>
            </div>

            <div class="form-group">
                <div class="form-group">
                    <label for="usr">JIRA cookie</label>
                    <input class="form-control" id="UserJiraCookie" name="UserJiraCookie" type="text" value="@Model.UserJiraCookie" />
                    <a target="_blank" href="~/Jira/Authentication">Get JIRA cookie</a>
                </div>
            </div>

            <button type="submit" class="btn btn-primary">Update</button>

         
        }
    </div>
</div>