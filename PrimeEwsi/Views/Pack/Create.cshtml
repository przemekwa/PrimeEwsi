@model PrimeEwsi.Models.PackModel

@{
    ViewBag.Title = "Download the full pack";

}

<h2>Create pack</h2>

<div class="panel panel-primary" style="width: 70%; margin-top: 20px">
    <div class="panel-heading">
        <h3 class="panel-title">Add Pack</h3>
    </div>
    <div class="panel-body">

        @using (Html.BeginForm())
        {
            @Html.ValidationSummary()

            <table>
                <tr >
                    <td>Teets</td>
                    <td>
                         @Html.EditorFor(m => m.Teets) <a href="#" onclick="AddText(this)">Teet-33535</a>
                    </td>
                </tr>
                <tr>
                    <td>Component</td>
                    <td>
                        @Html.EditorFor(m => m.Component) <a href="#" onclick='AddComponent("PRIME_baza_addon_live")'>AddonLive</a> <a href="#" onclick='AddComponent("PRIME_baza_wbk20")'>Wbk20</a>
                    </td>
                </tr>
                <tr>
                    <td>Projects</td>
                    <td>
                        @Html.EditorFor(m => m.ProjectId)
                    </td>
                </tr>
                <tr>
                    <td>Test environment</td>
                    <td>
                        @Html.EditorFor(m => m.TestEnvironment)
                    </td>
                </tr>

                <tr>
                    <td>Files</td>
                    <td>@Html.TextAreaFor(m => m.Files, new{ @rows=10, @cols=300}) </td>
                </tr>

            </table>

            <input type="submit" value="Download" name="action:Download"/>
            <input type="submit" value="Send IVEC" name="action:Send"/>

        }
    </div>
</div>




